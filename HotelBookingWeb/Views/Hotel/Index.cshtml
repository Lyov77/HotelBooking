@model List<HotelViewModel>
<!DOCTYPE html>
<html>
<head>
    <title>Отели</title>
</head>
<body>
    <h1>Список отелей</h1>
    <table>
        <thead>
            <tr>
                <th>Название</th>
                <th>Адрес</th>
                <!-- Добавьте другие необходимые заголовки -->
            </tr>
        </thead>
        <tbody>
            @foreach (var hotel in Model)
            {
                <tr>
                    <td>@hotel.Name</td>
                    <td>@hotel.City</td>
                    <!-- Добавьте другие необходимые поля -->
                </tr>
            }
        </tbody>
    </table>
</body>
</html>